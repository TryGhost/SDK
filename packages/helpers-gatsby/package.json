{
  "name": "@tryghost/helpers-gatsby",
  "version": "1.0.57",
  "repository": "https://github.com/TryGhost/SDK/tree/master/packages/helpers-gatsby",
  "author": "Ghost Foundation",
  "license": "MIT",
  "main": "./lib/index.js",
  "scripts": {
    "dev": "echo \"Implement me!\"",
    "test": "NODE_ENV=testing c8 --reporter text --reporter cobertura mocha './test/**/*.test.js'",
    "lint": "eslint . --ext .js --cache",
    "posttest": "yarn lint",
    "clean": "rimraf lib",
    "build": "yarn clean && babel src --out-dir lib --presets=@babel/preset-env,@babel/preset-react",
    "prepack": "yarn build"
  },
  "files": [
    "/lib"
  ],
  "eslintIgnore": [
    "lib/**"
  ],
  "publishConfig": {
    "access": "public"
  },
  "devDependencies": {
    "@babel/cli": "7.15.7",
    "@babel/core": "7.15.8",
    "@babel/preset-env": "7.15.8",
    "@babel/preset-react": "7.14.5",
    "c8": "7.10.0",
    "mocha": "7.2.0",
    "react": "16.14.0",
    "rimraf": "3.0.2",
    "should": "13.2.3",
    "sinon": "9.2.4"
  },
  "dependencies": {
    "@reach/router": "^1.2.1",
    "@tryghost/helpers": "^1.1.52",
    "gatsby-link": "^2.9.0",
    "prop-types": "^15.6.2"
  },
  "peerDependencies": {
    "react": "^16.9.0"
  }
}
