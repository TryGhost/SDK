{
  "name": "@tryghost/helpers-gatsby",
  "version": "2.1.1",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/TryGhost/SDK.git",
    "directory": "packages/helpers-gatsby"
  },
  "author": "Ghost Foundation",
  "license": "MIT",
  "main": "./lib/index.js",
  "scripts": {
    "dev": "echo \"Implement me!\"",
    "test": "NODE_ENV=testing c8 --src lib --reporter text --reporter cobertura mocha './test/**/*.test.js'",
    "lint": "eslint . --ext .js --cache",
    "posttest": "yarn lint",
    "clean": "rimraf lib",
    "build": "yarn clean && babel src --out-dir lib --presets=@babel/preset-env,@babel/preset-react",
    "prepack": "yarn build"
  },
  "files": [
    "/lib"
  ],
  "eslintIgnore": [
    "lib/**"
  ],
  "publishConfig": {
    "access": "public"
  },
  "devDependencies": {
    "@babel/cli": "7.28.6",
    "@babel/core": "7.29.0",
    "@babel/preset-env": "7.29.0",
    "@babel/preset-react": "7.28.5",
    "c8": "11.0.0",
    "mocha": "11.7.5",
    "react": "19.2.4",
    "rimraf": "6.1.3",
    "should": "13.2.3",
    "sinon": "21.0.1"
  },
  "dependencies": {
    "@gatsbyjs/reach-router": "2.0.1",
    "@tryghost/helpers": "^1.1.102",
    "gatsby-link": "5.16.0",
    "prop-types": "15.8.1"
  },
  "peerDependencies": {
    "react": "^18.1.0 || ^19.0.0"
  }
}
