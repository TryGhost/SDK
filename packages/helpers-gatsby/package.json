{
  "name": "@tryghost/helpers-gatsby",
  "version": "2.0.1",
  "repository": "https://github.com/TryGhost/SDK/tree/master/packages/helpers-gatsby",
  "author": "Ghost Foundation",
  "license": "MIT",
  "main": "./lib/index.js",
  "scripts": {
    "dev": "echo \"Implement me!\"",
    "test": "NODE_ENV=testing c8 --all --reporter text --reporter cobertura mocha './test/**/*.test.js'",
    "lint": "eslint . --ext .js --cache",
    "posttest": "yarn lint",
    "clean": "rimraf lib",
    "build": "yarn clean && babel src --out-dir lib --presets=@babel/preset-env,@babel/preset-react",
    "prepack": "yarn build"
  },
  "files": [
    "/lib"
  ],
  "eslintIgnore": [
    "lib/**"
  ],
  "publishConfig": {
    "access": "public"
  },
  "devDependencies": {
    "@babel/cli": "7.17.10",
    "@babel/core": "7.18.5",
    "@babel/preset-env": "7.18.2",
    "@babel/preset-react": "7.17.12",
    "c8": "7.11.3",
    "mocha": "10.0.0",
    "react": "18.1.0",
    "rimraf": "3.0.2",
    "should": "13.2.3",
    "sinon": "14.0.0"
  },
  "dependencies": {
    "@gatsbyjs/reach-router": "1.3.7",
    "@tryghost/helpers": "^1.1.71",
    "gatsby-link": "4.16.0",
    "prop-types": "^15.6.2"
  },
  "peerDependencies": {
    "react": "^18.1.0"
  }
}
