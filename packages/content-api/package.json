{
  "name": "@tryghost/content-api",
  "version": "1.11.21",
  "repository": "https://github.com/TryGhost/SDK/tree/main/packages/content-api",
  "author": "Ghost Foundation",
  "license": "MIT",
  "main": "dist/content-api.umd.js",
  "types": "dist/content-api.d.ts",
  "exports": {
    ".": {
      "import": "./dist/content-api.es.js",
      "require": "./dist/content-api.umd.js"
    }
  },
  "files": [
    "LICENSE",
    "README.md",
    "dist"
  ],
  "scripts": {
    "dev": "echo 'No dev script defined'",
