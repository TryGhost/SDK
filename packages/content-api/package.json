    "posttest": "yarn lint"
  },
  "publishConfig": {
    "access": "public"
  },
  "devDependencies": {
    "eslint-plugin-ghost": "3.4.0",
    "msw": "^2.2.14",
    "vite": "^5.2.10",
    "vite-plugin-dts": "^3.9.0",
    "vitest": "^1.5.3"
  },
  "type": "module"
